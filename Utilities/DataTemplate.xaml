<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:WindowsVirusScanningSystem.ViewModel"
                    xmlns:view="clr-namespace:WindowsVirusScanningSystem.View">

    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:DocumentScanningFunctionVM}">
        <view:DocumentScanningFunction />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:PEFileAnalysisVM}">
        <view:PEFileAnalysis />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:WpfHexEditorVM}">
        <view:WpfHexEditor/>
    </DataTemplate>
</ResourceDictionary>